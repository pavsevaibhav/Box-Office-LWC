<aura:component implements="flexipage:availableForAllPageTypes,lightning:isUrlAddressable" access="global">
    <aura:attribute type="String" name="movies"/>
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <aura:attribute type="boolean" name="isInitCompleted"/>    
    <div class="slds-card">
        <c:detailsPage movies="{!v.movies}"></c:detailsPage>
    </div>
</aura:component>	